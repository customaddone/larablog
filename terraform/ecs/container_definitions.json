[
  {
    "name": "nginx",
    "image": "183344260146.dkr.ecr.ap-northeast-1.amazonaws.com/nginx:latest",
    "cpu": 128,
    "memoryReservation": 256,
    "essential": true,
    "links": [
      "php"
    ],
    "portMappings": [
      {
        "hostPort": 80,
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "nginx",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "service"
      }
    }
  },
  {
     "name": "php",
     "image": "183344260146.dkr.ecr.ap-northeast-1.amazonaws.com/php:latest",
     "cpu": 128,
     "memoryReservation": 256,
     "essential": true,
     "logConfiguration": {
       "logDriver": "awslogs",
       "options": {
         "awslogs-group": "php",
         "awslogs-region": "ap-northeast-1",
         "awslogs-stream-prefix": "php"
       }
     }
   }
]
